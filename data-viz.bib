@article{kinneyClarificationSlopeScale2022,
  title = {A Clarification of Slope and Scale},
  author = {Kinney, Chad E. L.},
  date = {2022-01},
  journaltitle = {Behavior Modification},
  shortjournal = {Behav. Modif.},
  volume = {46},
  number = {1},
