  title = {Tailoring the Visual Communication of Climate Projections for Local Adaptation Practitioners in {{Germany}} and the {{UK}}},
  author = {Lorenz, Susanne and Dessai, Suraje and Forster, Piers M. and Paavola, Jouni},
